{
  "name": "rails-db-schema",
  "displayName": "Rails DB Schema",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.101.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:rails-db-schema.showRailsDbSchema"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "rails-db-schema",
          "title": "Rails Db Schema",
          "icon": "resources/activity-icon.svg"
        }
      ]
    },
    "menus": {
      "view/item/context": [
        {
          "command": "rails-db-schema.openInSchema",
          "when": "view == RailsDbSchema && viewItem == schemaTable",
          "group": "inline"
        }
      ]
    },
    "commands": [
      {
        "command": "rails-db-schema.showRailsDbSchema",
        "title": "Show Rails Db Schema"
      },
      {
        "command": "rails-db-schema.openInSchema",
        "title": "Open in schema",
        "icon": "$(go-to-file)"
      }
    ],
    "views": {
      "rails-db-schema": [
        {
          "id": "RailsDbSchema",
          "name": "Rails Db Schema",
          "icon": "resources/activity-icon.svg"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.101.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "eslint": "^9.25.1",
    "typescript": "^5.8.3",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2"
  }
}